      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.jsx",
        "options": {
          "jsxSingleQuote": true
        }
      }
    ]
  },
  "devDependencies": {
    "@types/styled-components": "^5.1.13"
